    <script type="text/javascript">
    function clamp(cl, height){
      var clamps = $(cl);
      console.log(clamps)
      for (var i = 0; i < clamps.length; i++) {
        var toClamp = clamps[i];
        while (toClamp.height > height) {
          console.log(toClamp.height)
            toClamp.text(function (index, text) {
                return text.replace(/\W*\s(\S)*$/, '...');
            });
        }
      }
    }

      $(document).ready(function(){
        $('.single-item').slick({
          dots: true,
          prevArrow:"<button type='button' class='slickprevnext' style='left:-7px; margin-top: 13px;'><img src='/img/left.png' style='width:13px; height: 25px;'/ ></button>",
          nextArrow:"<button type='button' class='slickprevnext' style='right:-7px; margin-top: 13px;'><img src='/img/right.png' style='width:13px; height: 25px;'/ ></button>",
          autoplay:true,
          autoplaySpeed:8000,
        });

        
        
      });

      $(window).load(function() {
        clamp('.clamp64', 64);
        clamp('.clamp106', 106)
      })
    </script>


